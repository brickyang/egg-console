{
  "name": "egg-console",
  "version": "1.1.3",
  "description": "Egg.js plugin to make your debug more easier.",
  "eggPlugin": {
    "name": "console"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "logger",
    "egg-logger"
  ],
  "dependencies": {
    "colors": "^1.1.2",
    "egg-logger": "^1.6.0"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "autod-egg": "^1.0.0",
    "egg": "^1.4.0",
    "egg-bin": "^3.4.0",
    "egg-ci": "^1.6.0",
    "egg-mock": "^3.7.0",
    "eslint": "^3.19.0",
    "eslint-config-egg": "^4.2.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
